{
  "jquery": "^3.3.1",
  "name": "nkod-tsa",
  "version": "1.0.0",
  "description": "Time series analysis for NKOD.",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --colors -p",
    "start-dev": "concurrently -n \"WEBPACK,FLASK\" \"npm run webpack-dev-server\" \"npm run flask-server\"",
    "start": "npm run build; npm run flask-prod",
    "webpack-dev-server": "NODE_ENV=debug webpack-dev-server --port 2992 --hot --inline",
    "flask-server": "FLASK_APP=$PWD/autoapp.py FLASK_DEBUG=1 flask run",
    "flask-prod": "gunicorn -k gevent -w 4 -b 0.0.0.0:8000 autoapp:app",
    "lint": "eslint \"assets/js/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eghuro/crawlcheckio.git"
  },
  "author": "Alexandr Mansurov",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/eghuro/crawlcheckio/issues"
  },
  "homepage": "https://github.com/eghuro/crawlcheckio#readme",
  "dependencies": {
    "jquery": "^3.3.1",
    "sync-exec": "^0.6.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "concurrently": "^3.5.0",
    "cryptiles": "^4.1.2",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "hoek": "^4.2.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "manifest-revision-webpack-plugin": "^0.4.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "url-parse": "^1.4.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
